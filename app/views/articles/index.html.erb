<div class="container"> 
<div class="content">
    <% @articles.each do |article| %>
        <% unless article.archived? %>
          <div class="article">
            <h2><%= link_to article.title, article %></h2>
            <p><%= article.body[0,201] %>...</p>
         </div>
        <% end %>
    <% end %>
</div>

</div>


